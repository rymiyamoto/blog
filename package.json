{
  "name": "vitepress-project",
  "private": true,
  "type": "module",
  "scripts": {
    "gen:page": "tsx docs/src/scripts/generatePages.ts",
    "gen:link": "tsx docs/src/scripts/autoLinkPosts.ts",
    "gen": "pnpm gen:link && pnpm gen:page",
    "dev": "pnpm gen && vitepress dev docs --host",
    "build": "pnpm gen && vitepress build docs"
  },
  "devDependencies": {
    "@types/node": "24.0.10",
    "tsx": "4.20.3",
    "vitepress": "1.6.3",
    "vue": "3.5.17"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    },
    "ignoredBuiltDependencies": [
      "esbuild",
      "vue-demi"
    ]
  },
  "dependencies": {
    "gray-matter": "4.0.3"
  }
}
