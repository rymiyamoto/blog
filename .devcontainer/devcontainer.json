{
    "name": "pandam-blog",
    "build": {
        "target": "dev",
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash"
            }
        }
    },
    "forwardPorts": [
        5173
    ],
    "postCreateCommand": "pnpm install",
    "workspaceFolder": "/workspace"
}
